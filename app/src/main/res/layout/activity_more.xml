<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".more"
    android:background="@drawable/homecolor"
    >

    <TextView
        android:layout_width="306dp"
        android:layout_height="67dp"
        android:text="Мини-приложения"
        android:textSize="30dp"
        tools:ignore="MissingConstraints"
        android:textStyle="bold"
        android:textColor="@color/white"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="16dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="328dp"
        android:layout_height="69dp"
        app:srcCompat="@drawable/botton"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="518dp" />

    <ImageButton
        android:id="@+id/homenavigationButton"
        android:layout_width="79dp"
        android:layout_height="67dp"
        app:srcCompat="@mipmap/ic_launcher14_round"
        tools:layout_editor_absoluteX="46dp"
        tools:layout_editor_absoluteY="520dp"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/assistantnavigationbutton"
        android:layout_width="83dp"
        android:layout_height="68dp"
        app:srcCompat="@mipmap/ic_launcher15_foreground"
        tools:layout_editor_absoluteX="151dp"
        tools:layout_editor_absoluteY="518dp"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/morenavigationbutton"
        android:layout_width="101dp"
        android:layout_height="31dp"
        app:srcCompat="@mipmap/ic_launcher16_foreground"
        tools:layout_editor_absoluteX="259dp"
        tools:layout_editor_absoluteY="537dp"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="116dp"
        android:layout_height="119dp"
        app:srcCompat="@mipmap/note_foreground"
        tools:layout_editor_absoluteX="46dp"
        tools:layout_editor_absoluteY="121dp" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="113dp"
        android:layout_height="115dp"
        app:srcCompat="@mipmap/brain_foreground"
        tools:layout_editor_absoluteX="234dp"
        tools:layout_editor_absoluteY="125dp" />

</androidx.constraintlayout.widget.ConstraintLayout>